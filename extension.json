{
    "name": "MathJax",
    "version": "1.0",
    "author": [
        "hbshim"
    ],
    "url": "https://github.com/hbshim/mediawiki-mathjax",
    "description": "Enables MathJax (http://www.mathjax.org/) for typesetting TeX and LaTeX formulae in MediaWiki",
    "license-name": "GPL v3",
    "AutoloadClasses": {
        "MathJax_Parser": "MathJax.php"
    },
	"Hooks": {
		"ParserFirstCallInit": ["MathJax_Parser::parserInit"],
        "BeforePageDisplay": ["MathJax_Parser::Inject_JS"]
	},
    "type": "Tag extensions",
	"manifest_version": 1
}
